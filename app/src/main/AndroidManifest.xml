<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.rain">
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/rain"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:supportsRtl="true"
        android:roundIcon="@mipmap/rain"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true"
        tools:ignore="GoogleAppIndexingWarning">
        <uses-library
            android:name="org.apache.http.legacy"
            android:required="false" />
        <activity android:name=".login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".newpassword"/>
        <activity android:name=".forgotpassword"/>
        <activity android:name=".signup"/>
        <activity android:name=".dashboard" />
        <activity android:name=".Appsactivity"/>
        <activity android:name=".Addcontact"/>
        <activity android:name=".caseGroupChat"/>
        <activity android:name=".case_setting"/>
        <activity android:name=".search"/>




    </application>

</manifest>